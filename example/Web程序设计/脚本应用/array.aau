	log = function(...){
		console.log(...) 
	}   
    [1] = "123";
    [2] = "456"; 
    item = function(index){
    	//external不能作为数组使用,
    	//所以我们需要自已实现一个接口访问数组成员
    	return owner[index+1] //注意AAuto下标从1开始,而Javascript从0开始
    } 
 }  
//在external中直接访问数组成员
