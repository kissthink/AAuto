//使用js数组
wb.external.testJsArray = function(jsArray){

		io.open()
		
		for(i=1;jsArray.getlength() ){
			io.print( i, jsArray[i-1] ) 
		}
}  
//在external中直接访问数组成员
var js = /*
	var jsArray = new Array('abc',123)
	external.testJsArray(jsArray) 
    */
wb.doScript( js );

